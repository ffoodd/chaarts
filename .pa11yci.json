{
	"standard": "WCAG2AA",
	"level": "warning",
	"concurrency": 1,
	"defaults": {
		"reporters": [
			[
				"pa11y-ci-reporter-html",
				{
					"destination": "./reports"
				}
			]
		],
		"wait": 3000,
		"runners": [
			"axe"
		],
		"timeout": 100000,
		"hideElements": "",
		"ignore": [
		],
		"defaults": {
			"chromeLaunchConfig": {
				"executablePath": "/usr/bin/google-chrome"
			}
		}
	}
}
